{
  "name": "zetcards-vscode",
  "displayName": "ZetCards VSCode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:zetcards.openCardPanel"
  ],
  "main": "./dist/ext",
  "contributes": {
    "commands": [
      {
        "command": "zetcards.openCardPanel",
        "title": "ZetCards: Open Card Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "(cd ../core; npm run compile;) && webpack",
    "watch": "webpack",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "core": "file:../core",
    "elasticlunr": "^0.9.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
